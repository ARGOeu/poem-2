{
  "name": "poem",
  "version": "3.0.0",
  "author": "Daniel Vrcic <dvrcic@srce.hr>, Katarina Zailac <kzailac@srce.hr>",
  "description": "React frontend for POEM Django web application",
  "main": "webpack.config.js",
  "scripts": {
    "build-devel": "webpack --config webpack.config.js --progress --colors --mode development",
    "build-prod": "webpack --config webpack.config.js --progress --colors --mode production",
    "watch": "webpack --config webpack.config.js --watch --mode development",
    "test": "jest --ci --reporters=default --reporters=jest-junit --coverage --coverageDirectory coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/frontend/react/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/frontend/react/__mocks__/fileMock.js"
    },
    "coverageReporters": [
      "cobertura",
      "text-summary"
    ],
    "testEnvironment": "jsdom"
  },
  "jest-junit": {
    "suiteName": "Frontend tests"
  },
  "browserslist": [
    "defaults"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ARGOeu/poem-2.git"
